package com.espn.mobile.xgames.model
{
	import com.espn.mobile.xgames.service.BaseService;

	/**
	 * Model to store all data related to the Twitter Authentication.
	 */	
	public class TwitterModel extends BaseService
	{
		private var _authToken:Object;
		
		/**
		 * Class constructor.
		 */	
		public function TwitterModel()
		{
		}
		
		/**
		 * Setter for authentication information.
		 */	
		public function set authToken(value:Object):void
		{
			_authToken = value;
		}
		
		/**
		 * Getter for authentication information.
		 */	
		public function get authToken():Object
		{
			return _authToken;
		}
	}
}