package com.espn.mobile.xgames.model.vo.alerts
{
	import com.espn.mobile.xgames.model.vo.XFavoriteVO;

	public class XAlertFavoriteVO
	{
		public var appId:String;
		public var type:String;
		public var keyName:String;
		public var favoriteId:String;
		
		public var sqliteObject:XFavoriteVO;
		
		public var itemName:String;
		
		/**
		 * Create a custom value object for event
		 * @param	appId:String=null
		 * @param	type:String=null
		 * @param	keyName:String=null
		 * @param	favoriteId:String=null
		 */
		public function XAlertFavoriteVO(appId:String=null, type:String=null, keyName:String=null, favoriteId:String=null,itemName:String=null):void
		{
			if(appId)this.appId = appId;
			if(type)this.type = type;
			if(keyName)this.keyName = keyName;
			if(favoriteId)this.favoriteId = favoriteId;
			if(itemName)this.itemName = itemName;
		}
	}
}