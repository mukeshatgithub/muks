package com.espn.mobile.xgames.model.vo
{
	[Bindable]
	[Table(name="Favorite")]
	[RemoteClass(alias="Favorite")]
	public class XFavoriteVO
	{		
		
		[Id] 
		/**
		 * Unique database entity id, will be set automatically.
		 */
		public var id:int;
		
		[Column(name="favId")] 
		public var favId:String;
		
		[Column(name="name")] 
		public var name:String;
		
		[Column(name="type")] 
		public var type:String;
		
		public var isFavorite:Boolean = true;
	}
}